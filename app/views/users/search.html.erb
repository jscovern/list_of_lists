<div class="container"> 
   <div class="col-md-offset-2 col-md-8">
        <% @users.each do |user| %>
        <div class="row">
            <h2><a href="/users/<%=user.id%>"><%= user.username %></a></h2>
            <h3> <%= user.first_name %> <%= user.last_name %></h3>
            <img src="<%= user.img_url %>" width=100%>
            <h3> <%= user.bio %></h3> 
        </div>
        <% end %>
    </div>
</div>