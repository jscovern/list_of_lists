<% @list.places.each do |place|%>
<div class="panel panel-default">
<div class="row list-item">
	<br />
	<div class="col-md-3">
		<img src="<%= place.img_url %>" width=100%>   
	</div>
	<div class="col-md-9">
		<h3><strong><%= place.name %></strong></h3><hr>
		<p><%= place.description %></p>
		<% if @list.user_ids.include?(session[:user_id]) %>
			<%= button_to "Delete", places_destroy_path(place), method: :delete %>
		<% end %>
	</div>    
</div>  
</div>
<%end%>