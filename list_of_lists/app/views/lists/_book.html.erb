<% @list.books.each do |book|%>
<div class="row list-item">
	<br />
	<div class="col-md-3">
		<img src="<%= book.img_url %>" width=100%>   
	</div>
	<div class="col-md-9">
		<h3><strong><%= book.title %></strong>, <%= book.author %></h3><hr>
		<p><%= book.description %>
	</div>    
</div>  

<%end%>